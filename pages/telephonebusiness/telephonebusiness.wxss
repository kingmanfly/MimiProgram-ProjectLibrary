.container {
  background: url('http://106.14.176.248/kingman/telephoneBusiness/images/background_commumication.png');
  background-size: cover;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 100%;
  height:100%;
}
.label-title{
    text-align: center;
    font-size: 2.5rem;
    color: white;
    padding-top: 20rpx;
}
.label-asfollow{
    text-align: center;
    font-size: 1.0rem;
    display: flex;
    justify-content: center;
    padding-top: 5rpx;
    padding-bottom: 10rpx;
    margin-bottom: 60rpx;
    color: white;
}
.lable-category1{
    font-size: 0.9rem;
    color: white;
    vertical-align: middle;
    margin-top: 15rpx;
    padding: 10rpx 10rpx 10rpx 15rpx;
    background:#eb8a00; 
}
.lable-category2{
    border-radius: 0% 5% 5% 0%; 
    font-size: 0.7rem;
    color: white;
    vertical-align: middle;
    background:#eb8a00; 
    padding: 5rpx;
    margin-top: 8rpx;
}
.business-content{
    display: flex; 
    border-radius: 2%; 
    border-color: #f30;
    border-style:solid;
    border-width:1px;
    width: 630rpx;
    font-size: 0.7rem;
    color: black;
    line-height:1.2em;
    align-items: center;
    margin-left: 1rem;
    margin-top: 0.5rem;
    margin-bottom: 0.5rem;
    padding: 0.3rem;
    background:white; 
}
.emphasize{
  color: red;
}
.contact {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: black;
  color: white;
  padding: 20rpx;
}

.img-phone {
  width: 60rpx;
  height: 60rpx;
}

.telephone contact-button{
  vertical-align: middle;
}

.telephone image{
  vertical-align: middle;
}